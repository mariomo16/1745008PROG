Algoritmo ejercicio4
	
	Escribir "Introduce número A";
	Leer a;
	
	Escribir "Introduce número B";
	Leer b;
	
	Escribir "Introduce número C";
	Leer c;
	
	Si (a > b) Entonces
		Escribir "A es el mayor.";
	Sino 
		Si (c > b) Entonces
			Escribir "C es el mayor.";
		SiNo
			Escribir "B es el mayor.";
		FinSi
	FinSi
	
FinAlgoritmo